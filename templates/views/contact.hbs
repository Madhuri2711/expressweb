	<!-- Start Contact -->
	<!--<div class="map-full"></div>-->
	<div class="contact-box">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="heading-title text-center">
							<h2>Contact</h2>
						<p></p>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-lg-12">
					<form id="">
						<div class="row">
							<div class="col-md-12">
								<div class="form-group">
									<input type="text" class="form-control" id="from_name" name="name" placeholder="Full Name" required >
									<div id="name_error_msg" style="margin-top: 10px;" class="help-block with-errors"></div>

								</div>                                 
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<input type="text" class="form-control" id="from_mob"  name="mob" placeholder="Phone Number" required>
									<div id="mob_error_msg" style="margin-top: 10px;" class="help-block with-errors"></div>

								</div>                                 
							</div>
							<div class="col-md-12">
								<div class="form-group">
									<input type="email" placeholder="Email Address" id="from_email" class="form-control" name="name" required >
									<div class="help-block with-errors" id="email_error_msg" style="margin-top: 10px;"></div>
								</div> 
							</div>
							
							<div class="col-md-12">
								<div class="form-group"> 
									<textarea class="form-control" id="from_msg" placeholder="Your Message" rows="4" data-error="Write your message"  required></textarea>
									<div class="help-block with-errors" id="msg_error_msg" style="margin-top: 10px;"></div>
								</div>
								<div class="submit-button text-center">
									<button class="btn btn-common" id="submit" type="submit">Send Message</button>
									<div id="msgSubmit" class="h3 text-center hidden"></div> 
									<div class="clearfix"></div> 
								</div>
							</div>
						</div>            
					</form>
				</div>
			</div>
		</div>
	</div>
	<!-- End Contact -->
	
	
